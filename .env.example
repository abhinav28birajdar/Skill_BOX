# SkillBox Environment Configuration
# Copy this file to .env and update with your actual values

# Supabase Configuration
# Get these from your Supabase project settings
EXPO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_BASE_URL=https://your-project-id.supabase.co/rest/v1

# Optional: Analytics and Monitoring
EXPO_PUBLIC_ANALYTICS_ENABLED=false
EXPO_PUBLIC_CRASHLYTICS_ENABLED=false

# Optional: AI Services (if using OpenAI or other AI providers)
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Optional: Push Notifications
EXPO_PUSH_TOKEN=your-expo-push-token-here

# Optional: Social Auth (if implementing social login)
GOOGLE_CLIENT_ID=your-google-client-id
APPLE_CLIENT_ID=your-apple-client-id

# Development Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug
